From b6266790b771b571d3104adfba7e804a939648d8 Mon Sep 17 00:00:00 2001
From: Chris Blake <chrisrblake93@gmail.com>
Date: Thu, 2 Apr 2020 09:26:37 -0500
Subject: [PATCH 06/21] ARM: dts: sun8i-h3: Add clock-frequency

To avoid error messages during boot.
---
 arch/arm/boot/dts/sun8i-h3.dtsi | 4 ++++
 1 file changed, 4 insertions(+)

diff --git a/arch/arm/boot/dts/sun8i-h3.dtsi b/arch/arm/boot/dts/sun8i-h3.dtsi
index 20217e2ca4d3..83cbdc213c99 100644
--- a/arch/arm/boot/dts/sun8i-h3.dtsi
+++ b/arch/arm/boot/dts/sun8i-h3.dtsi
@@ -78,6 +78,7 @@
 			clock-names = "cpu";
 			operating-points-v2 = <&cpu0_opp_table>;
 			#cooling-cells = <2>;
+      clock-frequency = <1200000000>;
 		};
 
 		cpu1: cpu@1 {
@@ -88,6 +89,7 @@
 			clock-names = "cpu";
 			operating-points-v2 = <&cpu0_opp_table>;
 			#cooling-cells = <2>;
+      clock-frequency = <1200000000>;
 		};
 
 		cpu2: cpu@2 {
@@ -98,6 +100,7 @@
 			clock-names = "cpu";
 			operating-points-v2 = <&cpu0_opp_table>;
 			#cooling-cells = <2>;
+      clock-frequency = <1200000000>;
 		};
 
 		cpu3: cpu@3 {
@@ -108,6 +111,7 @@
 			clock-names = "cpu";
 			operating-points-v2 = <&cpu0_opp_table>;
 			#cooling-cells = <2>;
+      clock-frequency = <1200000000>;
 		};
 	};
 
